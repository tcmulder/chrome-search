{
"manifest_version": 2,
  "name": "Instant Search",
  "version": "1.0",
  "description": "Search just by typing",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["jquery.js","highlighter.js","scripts.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
      "default_icon": "icon.png"
    }
}
